<div id="side-user-info" class="container-fluid">
  <p><span class="glyphicon glyphicon-user"></span> 관리자</p>
  <a class="btn btn-primary btn-block" href="#">로그아웃</a>
</div>
<nav id="side-nav" class="container-fluid navbar navbar-default">
  <ul class="nav navbar-nav">
  {% for item in navigator[:4] %}
    <li class="dropdown {% if item.category == service.category %}current-menu{% endif %}">
      <a class="dropdown-toggle" data-toggle="dropdown" href="{{ item.category.get_absolute_url() }}">{{ item.category.name }}</a>
      <ul class="dropdown-menu">
      {% for service in item.services %}
        <li><a href="{{ service.get_absolute_url() }}">{{ service.name }}</a></li>
      {% endfor %}
      </ul>
    </li>
  {% endfor %}
  </ul>
</nav>
